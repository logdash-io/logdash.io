{
  "name": "@logdash/hyper-ui",
  "version": "0.1.0",
  "description": "LogDash design system components and utilities built for Svelte 5 with DaisyUI and Tailwind CSS",
  "type": "module",
  "main": "./src/lib/index.ts",
  "types": "./src/lib/index.ts",
  "exports": {
    ".": "./src/lib/index.ts",
    "./styles": "./src/lib/styles/main.css",
    "./styles/*": "./src/lib/styles/*.css",
    "./types": "./src/lib/types/index.ts",
    "./features": "./src/lib/features/index.ts",
    "./presentational": "./src/lib/presentational/index.ts",
    "./utils": "./src/lib/utils/index.ts",
    "./types/*": "./src/lib/types/*.ts",
    "./features/*": "./src/lib/features/*.ts",
    "./presentational/*": "./src/lib/presentational/*.ts",
    "./utils/*": "./src/lib/utils/*.ts"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "dev": "echo 'Internal workspace package - no build needed'",
    "typecheck": "svelte-check --tsconfig ./tsconfig.json",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "svelte-kit sync || echo ''"
  },
  "keywords": [
    "design-system",
    "ui",
    "components",
    "svelte",
    "daisyui",
    "tailwind",
    "logdash"
  ],
  "author": "LogDash Team",
  "license": "MIT",
  "dependencies": {
    "axios": "catalog:",
    "bits-ui": "^2.14.4",
    "lucide-svelte": "catalog:",
    "luxon": "catalog:",
    "ts-pattern": "catalog:"
  },
  "peerDependencies": {
    "@sveltejs/kit": "*",
    "@sveltejs/vite-plugin-svelte": "*",
    "daisyui": "*",
    "svelte": "*",
    "svelte-check": "*",
    "tailwindcss": "*",
    "typescript": "*",
    "vite": "*"
  }
}
