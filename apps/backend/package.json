{
  "name": "starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "stripe": "stripe listen --forward-to localhost:3000/payments/stripe/webhook",
    "migrate-up": "npx migrate-mongo up",
    "migrate-clickhouse": "dotenv -e .env -- clickhouse-migrations migrate"
  },
  "dependencies": {
    "@clickhouse/client": "^0.2.10",
    "@logdash/js-sdk": "^1.0.9",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.16",
    "@nestjs/core": "^11.0.16",
    "@nestjs/event-emitter": "^3.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mongoose": "^11.0.1",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.0.4",
    "@redis/client": "^5.0.1",
    "axios": "^1.9.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express-basic-auth": "^1.2.1",
    "mongoose": "^8.10.1",
    "reflect-metadata": "^0.2.2",
    "resend": "^4.1.2",
    "rxjs": "^7.8.1",
    "stripe": "^17.7.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.0.16",
    "@testcontainers/clickhouse": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "clickhouse-migrations": "^1.0.4",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jest-date-mock": "^1.0.10",
    "mongodb-memory-server": "^10.1.3",
    "nock": "^14.0.1",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "testcontainers": "^11.0.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "optionalDependencies": {
    "redis-memory-server": "^0.12.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "jest-date-mock"
    ],
    "globalSetup": "<rootDir>/test/utils/global-setup.ts",
    "globalTeardown": "<rootDir>/test/utils/global-teardown.ts",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    }
  }
}
