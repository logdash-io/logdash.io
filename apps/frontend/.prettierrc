{
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 80,
  "semi": true,
  "proseWrap": "preserve",
  "endOfLine": "lf",
  "tabWidth": 2,
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "ignore",
  "plugins": [
    "prettier-plugin-svelte"
  ],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ]
}
